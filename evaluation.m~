clear;
close all;
clc;

sl = load('srLoc.txt');
sl = sl(:,4);
slSum = sum(sl)

rl = load('rrLoc.txt');
rl = rl(:,4);
rlSum=sum(rl)

slng = load('sLoc-no-gps.txt');
slng = slng(:,4);
slngSum = sum(slng)

rlng = load('rLoc-no-gps.txt');
rlng = rlng(:,4);
rlngSum=sum(rlng)

alng = load('ML-area.txt');
alng = alng(:,4);
alngSum=sum(alng)

figure('Position',[440 378 560 620/3]);
bar([slSum slngSum ; rlSum rlngSum; 0 alngSum ] );
ylabel('Joule');
labels = {'S','RL' , 'A'};
set(gca , 'xticklabel' , labels);
set(gca , 'fontsize' , 16);
set(gca , 'xLim', [0.5 2.5])


figure('Position',[440 378 560 620/3]);
bar([length(sl) length(rl)  ] );
ylabel('Number of queries');
labels = {'SL','RL'};
set(gca , 'xticklabel' , labels);
set(gca , 'fontsize' , 16);
set(gca , 'xLim', [0.5 2.5])